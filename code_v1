$(document).ready(function(){$("#menu-toggle").click(function(t){t.preventDefault(),$("#wrapper").toggleClass("toggled")}),$("#main-content").on("click","#btnClearLog",function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer");$.ajax({url:t,data:{btnClearLog:"true"},dataType:"json",timeout:5e3,success:function(t){$("#main-content #LogList").html("<li class='list-group-item'>Nenhum Registro de Falha Localizado</li>")}}).error(function(t,a,e){alert("Não foi Possível Excluir os registros!, tente reiniciar o equipamento.")})}),$("#main-content").on("click","#btnResetTblStatus",function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer");$.ajax({url:t,data:{ResetTblStatus:"true"},dataType:"json",timeout:5e3,success:function(t){}}).error(function(t,a,e){alert("Não foi Possível Executar Este Comando!, Verifique a conexão com a internet.")})}),$("#main-content").on("click","#btnCtrl1",function(t){$("#txtBtnModal").val("1"),$("#btnControlModal").modal("show")}),$("#main-content").on("click","#btnCtrl2",function(t){$("#txtBtnModal").val("2"),$("#btnControlModal").modal("show")}),$("#main-content").on("click","#btnCtrl3",function(t){$("#txtBtnModal").val("3"),$("#btnControlModal").modal("show")}),$("#main-content").on("click","#btnCtrl4",function(t){$("#txtBtnModal").val("4"),$("#btnControlModal").modal("show")}),$("#main-content").on("submit","form",function(t){t.preventDefault();var a=$(this).serialize();$("#btnControlModal").modal("hide");var e="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&"+a,n=$(this).prev();$.ajax({url:e,dataType:"json",timeout:5e3,success:function(t){1==t.value?$(n).html("<div class='alert alert-success'><strong>Sucesso!</strong> Os Dados Foram Salvos.</div>"):$(n).html("<div class='alert alert-danger'><strong>Falha!</strong> Não foi Possível Salvar Os Dados.</div>"),$(this).prev().show(),setTimeout(function(){$(".alert").hide()},3e3)}}).error(function(t,a,e){$(n).html("<div class='alert alert-danger'><strong>Falha TimeOut!</strong> Servidor N�o Respondeu.</div>")})});var t=null;t=setInterval(function(){var a=$("#ipBuffer").attr("data-ipBuffer"),e=$("#ipBuffer").attr("data-idBuffer");$.get("http://"+a+"/?Session="+e+"&GetStatus=true",function(e,n){if(0==e.login)return clearInterval(t),void $("#dvmsg").html("<div class='alert alert-warning'><strong>Login inválido!</strong> Por Favor Efetue O Login Para Continuar <a href='http://"+a+"' class='alert-link'>Logar Novamente</a></div>");$("#ResetRede1").text(e.rstRede1),$("#ResetRede2").text(e.rstRede2),$("#ResetRede3").text(e.rstRede3),$("#ResetRede4").text(e.rstRede4),1==e.statusRede1?$("#StatusRede1").html("<h4><span class='label label-success'>On</span></h4>"):$("#StatusRede1").html("<h4><span class='label label-danger'>Off</span></h4>"),1==e.statusRede2?$("#StatusRede2").html("<h4><span class='label label-success'>On</span></h4>"):$("#StatusRede2").html("<h4><span class='label label-danger'>Off</span></h4>"),1==e.statusRede3?$("#StatusRede3").html("<h4><span class='label label-success'>On</span></h4>"):$("#StatusRede3").html("<h4><span class='label label-danger'>Off</span></h4>"),1==e.statusRede4?$("#StatusRede4").html("<h4><span class='label label-success'>On</span></h4>"):$("#StatusRede4").html("<h4><span class='label label-danger'>Off</span></h4>")},"json")},3e3),$("#link_ctrlcontrole").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina1.htm";$("#main-content").load(t)}),$("#link_ping").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina2.htm";$("#main-content").load(t)}),$("#link_watchdog").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina3.htm";$("#main-content").load(t)}),$("#link_inter_controle").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina4.htm";$("#main-content").load(t)}),$("#link_rede").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina5.htm";$("#main-content").load(t)}),$("#link_senha").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina6.htm";$("#main-content").load(t)}),$("#link_assistencia").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina7.htm";$("#main-content").load(t)}),$("#link_log").click(function(){var t="http://"+$("#ipBuffer").attr("data-ipBuffer")+"/?Session="+$("#ipBuffer").attr("data-idBuffer")+"&pagina8.htm";$("#main-content").load(t)})});
